УСТРОЙСТВА                   ВВОДИМЫЙ ПАРАМЕТР

Adaptec 151x, 152x           aha152x.aha152x=iobase[,irq[,scsi-id[,reconnect]]]
Adaptec 1542                 aha1542.aha1542=iobase[,buson,busoff[,dmaspeed]]
